{
  "name": "walletconnect-web3-subprovider-browser",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rm -rf build_dev",
    "clean:cache": "rm -rf .cache",
    "clean-build": "rm -rf build",
    "dev": "npm run clean && parcel src/index.html --out-dir build_dev/",
    "build": "npm run clean-build && parcel build src/*.html --out-dir build/",
    "build:product": "npm run clean-build && parcel build --no-source-maps src/*.html --out-dir build/",
    "deploy": "cp build/scripts*.map dist/walletconnect-web3-subprovider.umd.js.map && cp build/scripts*.js dist/walletconnect-web3-subprovider.umd.js",
    "deploy:product": "cp build/scripts*.js dist/walletconnect-web3-subprovider.umd.min.js"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "gh-pages": "^2.1.1",
    "parcel-bundler": "^1.12.3"
  },
  "browserslist": [
    ">0.25%",
    "ie 11",
    "not op_mini all"
  ],
  "engines": {
    "node": "^10.16.0",
    "npm": "^6.10.3"
  },
  "dependencies": {
    "@walletconnect/web3-subprovider": "^1.0.0-beta.38"
  }
}
